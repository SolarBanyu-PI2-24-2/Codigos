                        <iframe src="//www.youtube.com/embed/93O0MD2ZzUs?si=iP1eZbuOPd8iu3qB" frameborder="0" allowfullscreen></iframe>
                    </div>
                    <button class="carousel-btn next"><i class="fas fa-chevron-right"></i></button>
                </div>
            </div>

            <!-- Carrossel de Tutoriais Escritos -->
            <div class="carousel">
                <h2>Tutoriais escritos</h2>
                <div class="carousel-container">
                    <button class="carousel-btn prev"><i class="fas fa-chevron-left"></i></button>
                    <div class="carousel-track">
                        <div class="tutorial-card">
                            <h3>Trocando o filtro</h3>
                            <a href="#" class="view-btn">Visualizar</a>
                        </div>
                        <div class="tutorial-card">
                            <h3>Adicionando uma nova unidade</h3>
                            <a href="#" class="view-btn">Visualizar</a>
                        </div>
                        <div class="tutorial-card">
                            <h3>Compartilhando um relatório</h3>
                            <a href="#" class="view-btn">Visualizar</a>
                        </div>
                        <div class="tutorial-card">
                            <h3>Utilizando a energia solar</h3>
                            <a href="#" class="view-btn">Visualizar</a>
                        </div>
                        <div class="tutorial-card">
                            <h3>Pedindo suporte</h3>
                            <a href="#" class="view-btn">Visualizar</a>
                        </div>
                        <div class="tutorial-card">
                            <h3>Ajudas extras</h3>
                            <a href="#" class="view-btn">Visualizar</a>
                        </div>
                    </div>
                    <button class="carousel-btn next"><i class="fas fa-chevron-right"></i></button>
                </div>
            </div>
        </div>
    </div>
    <script src="/js/scripts.js"></script>
